{
  "name": "11ty-liquid-optimized",
  "version": "1.0.0",
  "description": "11ty Liquid Optimized Website",
  "scripts": {
    "clean": "rm -rf _site",
    "build": "npm run clean && eleventy",
    "serve": "eleventy --serve",
    "quiet": "eleventy --serve --quiet",
    "dev": "npm run clean && npm run serve",
    "start": "npm run clean && npm run quiet",
    "build:prod": "npm run clean && NODE_ENV=production eleventy",
    "start:prod": "npm run clean && NODE_ENV=production eleventy --serve"
  },
